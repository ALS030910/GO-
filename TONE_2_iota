package main

import "fmt"

const (
	a = iota // a = 0
	b        // b = 1
	c        // c = 2
	d = 5    // d = 5
	e        // e = 5
	f        // f = 5
)
const (
	Apple, Banana     = iota + 1, iota + 2 // Apple=1 Banana=2
	Cherimoya, Durian                      // Cherimoya=2 Durian=3
	Elderberry, Fig                        // Elderberry=3, Fig=4

)

func main() {
	fmt.Println(a, e, f)
}
